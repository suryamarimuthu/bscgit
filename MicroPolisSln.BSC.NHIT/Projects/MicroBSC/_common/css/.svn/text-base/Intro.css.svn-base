.GridRowStyleIntro 
{ 
	vertical-align: middle; 
	border-right: solid 0px #BCC994; 
	border-bottom: solid 0px #BCC994; 
	text-align: left; 
	color: #333333; 
	background-color: #FFFFFF; 
	padding-left: 3px; 
	padding-right: 3px; 
	overflow: hidden; 	
}

.GridRowAlternateStyleIntro 
{ 
	background-color: #F9F9F9; 
	
	border-right: solid 0px #BCC994; 
	border-bottom: solid 0px #BCC994; 
	text-align: left; 
	color: #333333; 
	
	padding-left: 3px; 
	padding-right: 3px; 
	overflow: hidden; 
	vertical-align: middle; 
}

.GridSelectedRowStyleIntro 
{ 
	border-color: #C1CED4; 
	background-color: #EBF2E8;
}


.GridHeaderStyleIntro 
{ 
	background-color:#c6ddf5; 
	font-family:Dotum; 
	font-size:12px; 
	font-weight:bold; 
	color:#7a95c3;
	border-top:solid 1px #b0c4dc;
	border-bottom:solid 1px #b0c4dc;
	/*
    border-style:solid;
    border-width:1px; 
    border-color:#BCC994;
	*/
	height: 23px; 
	overflow: hidden; 
	padding-left: 3px; 
	padding-right: 3px; 
	text-align: center; 
	vertical-align: middle;
}

.GridRowSelectorStyleIntro 
{ 
	vertical-align: middle; 
	border: solid 0px #C1CED4; 
	text-align: center; 
	color: #333333; 
	width : 10px;
	background-color: #FFFFFF; 	
}


.GridFrameStyleIntro 
{ 	
    border-collapse:collapse;
	border: solid 0px #BCC994;
	background-color: #FFFFFF; 
	width: 100%; 
	height: 100%;
}
