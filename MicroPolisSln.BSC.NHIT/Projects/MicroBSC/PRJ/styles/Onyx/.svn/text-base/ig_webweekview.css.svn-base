.igwv_WeekViewFrame {
	margin-top: 2px;
	background-color: #FFFFD5;
}

.igwv_CaptionHeader {
	background-image: url(images/MonthHeaderCaption_bg.png);
	background-repeat: repeat-x;
	font-family: Tahoma, Verdana, Arial, Helvetica, sans-serif;
	font-size: x-small;
	font-weight: bold;	
	color: #FFFFFF;
	height: 25px;
	line-height: 25px;
	padding-top: 0px;
	padding-bottom: 0px;
	text-align: left;
	overflow: hidden;
	
	border-collapse: collapse;	
	border-bottom: 1px solid #4B4B4B;	
	border-left: 1px solid #353635;
	border-right: 1px solid #353635;
	background-color: #4B4B4B;
}

.igwv_NavigationButton { 
	border: 0px;
	width: 17px;
	background-color: transparent;
	vertical-align: middle;
	cursor: hand;	
}
	
.igwv_DayHeader{
	text-align: right;
	height:18px;
	padding: 0px 2px 0px 0px;
	cursor:default;
	font-size: 8pt;
	font-family: Tahoma, Verdana, Arial, Helvetica, sans-serif;
}

.igwv_TodayHeader {
	background-image: url(images/TodayHeader_bg.png);
	background-repeat: repeat-x;
	background-position: bottom;
	background-color: #FFFFD5;
}
.igwv_ActiveDayHeader 
{
	background-image: none;
	background-color: #FFFFFF;
	background-image: url(images/ActiveDayHeader_bg.png);
	background-repeat: repeat-x;
	background-position: bottom;
}

.igwv_Day {
	font-family: Microsoft Sans Serif; 
	font-size: 8pt;
	overflow-x:hidden;	
}

.igwv_DayArea {
	border: 1px solid #000000;
	background-color: #FFFFFF;
	background-image: url(images/cell_bg.png);
	background-repeat: repeat-x;
	background-position: bottom;	
}
		
.igwv_ActiveDay { 

}

.igwv_Appointment { 
	line-height:150%;
	white-space:nowrap;
	cursor: pointer;
	padding: 2px;	
	margin-right: 1px;
	margin-left: 1px;
}

.igwv_SelectedAppointment { 
	background-color: #FFA013;
	margin-top: 1px;
	padding: 1px;
	color: #FFFFFF;
	cursor: pointer;
	margin-left:1px;
	margin-right: 1px;
}

.igwv_ReminderIcon {
	background-image: url(/ig_common/images/reminder.gif);
	display: inline;
	margin-right: 5px;
	margin-left: 3px;
	width: 16px;
	height: 13px;	
}

.igwv_AllDayEvent { 
	border-style:solid;
	border-color:black;
	border-width: 1px;
	background-color: lightyellow;
}